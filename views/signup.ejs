<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("./partials/head") %>
    <title>Home Page</title>
  </head>
  <body>
    <%- include("./partials/nav") %>

    <form action="/user/signup" method="post">
      <div class="mb-3">
        <label
          for="fullName"
          class="form-label"
          >FullName</label
        >
        <input
          type="name"
          name="fullName"
          class="form-control"
          id="fullName"
         required
        />
          <div class="mb-3">
        <label
          for="email"
          class="form-label"
          >Email address</label
        >
        <input
          type="email"
          name="email"
          class="form-control"
          id="email"
          aria-describedby="emailHelp"
        />
        <div
          id="emailHelp"
          class="form-text"
        >
          We'll never share your email with anyone else.
        </div>
      </div>
      <div class="mb-3">
        <label
          for="exampleInputPassword1"
          class="form-label"
          >Password</label
        >
        <input
          type="password"
          name="password"
          class="form-control"
          id="exampleInputPassword1"
        />
      </div>
     
      <button
        type="submit"
        class="btn btn-primary"
      >
        Submit
      </button>
    </form>

    <%- include("./partials/script") %>
  </body>
</html>
